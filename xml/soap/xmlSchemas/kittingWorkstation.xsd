        <xsd:sequence>
          <xsd:element name="ObjectOnTable"
            type="SolidObjectType"
            minOccurs="0"
            maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

