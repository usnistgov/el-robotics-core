      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StateRelationType"/>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="UnderWithContactType">
    <xsd:annotation>
      <xsd:documentation>
	UnderWithContactType is derived from StateRelationType.
	UnderWithContactType consists of:
