	- RCC8Set (inherited)
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StateRelationType"/>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="InContactWithType">
    <xsd:annotation>
      <xsd:documentation>
